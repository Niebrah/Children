<?xml version = "1.0" encoding = "UTF-8"?>




<beans xmlns = "http://www.springframework.org/schema/beans" 
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	
	<bean id = "HelloWorld5" class = "com.tutorialspoint.HelloWorld5">
		<property name = "messageOriginal" value = "Hello World" />
		<property name = "messageNew" value = "Hello World2" />
	</bean>
	
	
	<bean id = "HelloMars" class = "com.tutorialspoint.HelloMars" parent = "HelloWorld">
		<property name = "messageOriginal" value = "Hello Earth" />
		<property name = "messageNew" value = "Hello Earthlings" />
		<property name = "messageNew2" value = "Hello Earthlings2" />
	</bean>


</beans>